{
    "version": 2,
    "builds": [
      {
        "src": "index.js",
        "use": "@vercel/node"
      }
    ],
    "routes": [
      {
        "src": "/(.*)",
        "dest": "index.js"
      }
    ],
    "env": {
      "MONGO_USERNAME": "@MONGO_USERNAME",
      "MONGO_PASSWORD": "@MONGO_PASSWORD",
      "MONGO_SERVER": "@MONGO_SERVER",
      "MONGO_TUNE_DB": "@MONGO_TUNE_DB"
    }
  }
  